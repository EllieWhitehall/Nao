    "import socket,pickle\n",
    "import time\n",
    "import naoqi\n",
    "from naoqi import ALProxy\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "''' DEFINITIONS '''\n",
    "\n",
    "def get_dataframe(file_path):\n",
    "    d = pd.read_csv(file_path, header=None, names=C)\n",
    "    return d\n",
    "\n",
    "def get_column(file_path, column):\n",
    "    d = pd.read_csv(file_path, header=None, names=C)\n",
    "    col = pd.DataFrame(d, columns = [column])\n",
    "    return col\n",
    "\n",
    "def std(file_path, column):\n",
    "    d = pd.read_csv(file_path, header=None, names=C)\n",
    "    col = pd.DataFrame(d, columns = [column])\n",
    "    std = np.std(col)\n",
    "    return std\n",
    "\n",
    "def mean(file_path, column):\n",
    "    d = pd.read_csv(file_path, header=None, names=C)\n",
    "    col = pd.DataFrame(d, columns = [column])\n",
    "    mean = np.mean(col)\n",
    "    return mean\n",
    "\n",
    "def graph(column1, column2):\n",
    "    col1 = np.array(get_column(file_path, column1))\n",
    "    col2 = np.array(get_column(file_path, column2))\n",
    "    plt.figure(figsize=(15,8))\n",
    "    plt.grid()\n",
    "    plt.xlabel(column1)\n",
    "    plt.ylabel(column2)\n",
    "    return plt.plot(col1, col2)"
